   $(document).ready(function($){

		$('.insight-tab').addClass('active');

		//$.hideLoading();

       $('.list-group-item1').find('input:radio').click(function(){
          $('input[type=radio]:checked').parent().addClass('active');
          $('input[type=radio]:not(:checked)').parent().removeClass('active');
            // declaring an array
            var choices1 = [];
            $('input[type=radio]:checked').each(function() {
               var resource_id1=jQuery(this).attr('value'); //alert(resource_id);
					choices1.push(resource_id1);
            });
            console.log(choices1);
			  //$.showLoading({ name: 'circle-fade'}); 
            $.ajax({
                url: "/wp-admin/admin-ajax.php",
                type :'POST',
                data : {
                    'action' : 'success_stories', // the php name function
                    'choices1' : choices1,
                },
                success: function (result) {
					//$.hideLoading();
					 $('.filter-output1').css('display','none');
					  $('.filter-output2').css('display','none');
					  $('.ajaxContent').css('display','block');
					  $('#insight .resource-download').hide();
					  $('#case-study .resource-download').hide();
					  $('.filter-output').show();
                 $('.filter-output').empty();
                 $('.filter-output').append(result);
                  // just for test - success (you can remove it later)
                  //console.log(result);
                  //console.log(choices);
                },
                error: function(err){
                    // just for test - error (you can remove it later)
                    console.log(err);
                    console.log(choices1);
                },
				   complete: function() {
                  $.getScript("https://www.damcogroup.com/wp-content/themes/DamcoNew/js/custom-new.js", function() {
               });
            }
         });
      })
   });

   AOS.init();
});</script>
